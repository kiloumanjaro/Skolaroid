# Database configuration
# Connect to Supabase via connection pooling
DATABASE_URL=postgresql://user:password@aws-region.pooler.supabase.com:6543/postgres?pgbouncer=true

# Direct connection to the database (used for migrations)
DIRECT_URL=postgresql://user:password@aws-region.pooler.supabase.com:5432/postgres

# Supabase configuration
# Update these with your Supabase details from your project settings > API
# https://app.supabase.com/project/_/settings/api
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_KEY=your-publishable-or-anon-key

# Vercel configuration (for CI/CD)
VERCEL_OIDC_TOKEN=your-vercel-oidc-token
